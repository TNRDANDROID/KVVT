                            android:layout_marginRight="15dp"
                            android:background="@drawable/background"
                            android:visibility="visible" />

                        <LinearLayout
                            android:id="@+id/natham_land_layout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:visibility="visible">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="8"
                                android:gravity="center"
                                android:layout_gravity="center"
                                android:orientation="vertical">

                                <TextView

                                    style="@string/font_circular_book"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:padding="5dp"
                                    android:text="@string/natham_land_status"
                                    android:textAlignment="viewEnd"
                                    android:textColor="#000"
                                    android:textSize="15sp" />

                            </LinearLayout>


                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center_horizontal"
                                android:orientation="vertical">

                                <TextView

                                    style="@string/font_circular_book"
                                    android:layout_width="wrap_content"

                                    android:layout_height="wrap_content"
                                    android:padding="10dp"
                                    android:text=":"
                                    android:textColor="#000"
                                    android:textSize="15sp" />

                            </LinearLayout>


                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="8"
                                android:gravity="center"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/natham_land_status_tv"
                                    style="@string/font_circular_book"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:padding="10dp"
                                    android:text="Y"
                                    android:textAlignment="inherit"
                                    android:textColor="#000"
                                    android:textSize="15sp" />
                            </LinearLayout>
                        </LinearLayout>


                        <LinearLayout
                            android:id="@+id/beneficiary_photos_layout"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_100sdp"
                            android:orientation="horizontal"
                            android:weightSum="2"
                            android:visibility="visible">

                            <RelativeLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1">
                                <ImageView
                                    android:scaleType="fitXY"
                                    android:layout_margin="@dimen/_5sdp"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:id="@+id/benificiery_image"></ImageView>
                            </RelativeLayout>
                            <RelativeLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1">
                                <ImageView
                                    android:scaleType="fitXY"
                                    android:layout_margin="@dimen/_5sdp"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:id="@+id/benificiery_house_image"></ImageView>

                            </RelativeLayout>

                        </LinearLayout>


                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/_3sdp"
                            android:background="@drawable/bottom_curve_background"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/upload"
                                android:layout_width="90dp"
                                android:layout_height="40dp"
                                android:layout_alignParentStart="true"
                                android:gravity="center"
                                android:text="@string/upload"
                                android:textAlignment="center"
                                android:textColor="@android:color/white"
                                android:textSize="15sp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/delete"
                                android:layout_width="90dp"
                                android:layout_height="40dp"
                                android:layout_alignParentEnd="true"
                                android:gravity="center"
                                android:text="@string/delete"
                                android:textAlignment="center"
                                android:textColor="@android:color/white"
                                android:textSize="15sp"
                                android:textStyle="bold" />
                        </RelativeLayout>


                    </LinearLayout>


                </LinearLayout>

            </RelativeLayout>

        </androidx.cardview.widget.CardView>

    </RelativeLayout>
</layout>